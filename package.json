{
  "name": "syde322-a2",
  "version": "0.1.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "NODE_ENV=dev node dist",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "prebuild": "webpack --config webpack.config.js -p",
    "start": "node dist",
    "postinstall": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developit/express-es6-rest-api.git"
  },
  "author": "Manfred Cheung <mj3cheun@uwaterloo.ca>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "bootstrap": "^3.3.7",
    "compression": "^1.5.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.9-1",
    "express": "^4.13.3",
    "localforage": "^1.6.0",
    "lodash": "^4.17.5",
    "material-ui": "^0.20.0",
    "morgan": "^1.8.0",
    "peer": "^0.2.8",
    "peerjs": "^0.3.14",
    "pg-promise": "^8.2.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router-dom": "^4.0.0",
    "react-table": "^6.8.0",
    "resource-router-middleware": "^0.6.0",
    "spinkit": "^1.2.5",
    "uniqid": "^4.1.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.1.1",
    "file-loader": "^0.11.0",
    "nodemon": "^1.9.2",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack": "^2.2.1"
  },
  "homepage": "https://github.com/developit/express-es6-rest-api#readme"
}
